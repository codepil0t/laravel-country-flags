{
    "name": "codepil0t/laravel-country-flags",
    "description": "A Laravel package for country flags",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Fedoruk",
            "email": "darkblackhack@gmail.com"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/codepil0t/laravel-country-flags.git"
    },
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Codepil0t\\LaravelCountryFlags\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codepil0t\\LaravelCountryFlags\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codepil0t\\LaravelCountryFlags\\FlagsServiceProvider"
            ]
        }
    }
}
